{
  "name": "ai-im-agent-backend-build",
  "description": "Build and deploy AI IM Agent Backend to ECR",
  "source": {
    "type": "GITHUB",
    "location": "https://github.com/AI-Empower-360/ai-orchestrator.git",
    "buildspec": "buildspec.yml",
    "gitCloneDepth": 1
  },
  "artifacts": {
    "type": "NO_ARTIFACTS"
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/standard:7.0",
    "computeType": "BUILD_GENERAL1_MEDIUM",
    "privilegedMode": true,
    "environmentVariables": [
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "us-east-1"
      },
      {
        "name": "AWS_ACCOUNT_ID",
        "value": "996099991638"
      },
      {
        "name": "IMAGE_REPO_NAME",
        "value": "ai-im-agent-backend"
      },
      {
        "name": "IMAGE_TAG",
        "value": "production"
      },
      {
        "name": "UPDATE_ECS_SERVICE",
        "value": "true"
      }
    ]
  },
  "serviceRole": "arn:aws:iam::996099991638:role/codebuild-ai-im-agent-service-role",
  "logsConfig": {
    "cloudWatchLogs": {
      "status": "ENABLED",
      "groupName": "/aws/codebuild/ai-im-agent-backend-build"
    }
  }
}
